package com.trupti;

public class MainApp {
	public static void main(String[] args) {
		ThingsInClass thingsInClass = new ThingsInClass();
		ThingsInClass thingsInClass2 = new ThingsInClass();
		
		int x = thingsInClass.x;
		System.out.println(x);
		thingsInClass.x = 50;
		System.out.println(x);	
		System.out.println(thingsInClass2.x);
	}
}
