package com.trupti;

public class StringDemo {
	public static void main(String[] args) {
		
		// Strings are immutable
		String s = "Trupti"; // creates a object in heap then it pushes it to String literal pool
		String s1 = "Trupti"; // this references to string in pool
		String s2 = new String("Trupti"); // This create new object in heap
		String s7 = new String("abc"); // create object in heap
		String s8 = s7.intern(); // pushes abc into pool
		String s9 = "abc"; // references to abc in pool
		String s3 = "Balbudhe"; // creates object in heap then pushes it to pool
		String s4 = "Trupti" + "Balbudhe"; // concatination at compile time and stores it in pool
		String s5 = "TruptiBalbudhe"; // references to string in pool
		String s6 = s1 + s3; // evaluates at run time so creates a new object in heap
		
		System.out.println(s5 == s4);
		
//		System.out.println(s);
//		String s2 = s.toUpperCase(); // This will not change s
//		System.out.println(s2);
	}
}
