package com.trupti;

public class DataTypeDemo {
	public static void main(String[] args) {
		// By default all data types are signed
		
		byte a = 1; // 127 to -128
		short b = 2; // here 2 is of integer type then it gets type casted, this applies for byte, short and long
		int c = 4;
		long d = 8888888888888888888L;
		
		float e = 2.5f;
		double f = 32.44;
		
		byte a1 = 111;
		byte b2 = 121;
		byte b3 = (byte)(a1 + b2);
		System.out.println(b3);
		System.out.println(a1 + b2);
		
		char g = 'A';
		int gg = g;
		System.out.println(g);
		System.out.println(gg);
	}
}
