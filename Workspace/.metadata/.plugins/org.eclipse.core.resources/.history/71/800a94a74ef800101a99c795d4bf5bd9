package com.trupti;

public class AbstractDemo {
	public static void main(String[] args) {
		House myHouse = new MyHouse();
		House yourHouse = new YourHouse();
		displayHouse(myHouse);
		displayHouse(yourHouse);
	}
	
	public static void displayHouse(House house) {
		house.kitchen();
		house.livingRoom();
		house.washRoom();
		house.garden();
	}
}
