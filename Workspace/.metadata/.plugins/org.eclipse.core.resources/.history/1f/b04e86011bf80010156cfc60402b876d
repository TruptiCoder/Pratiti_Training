package com.trupti;

public class MainApp {
	public static void main(String[] args) {
		ThingsInClass thingsInClass = new ThingsInClass();
		ThingsInClass thingsInClass2 = new ThingsInClass();

		ThingsInClass t1 = new ThingsInClass();
		ThingsInClass.A a = t1.new A();
		
		System.out.println(a.xaa);
		
		int x = thingsInClass.x;
		System.out.println(x);
		thingsInClass.x = 50;
		System.out.println(thingsInClass.x);	
		System.out.println(thingsInClass2.x);
		
		System.out.println(ThingsInClass.A.xaa);
	}
}
