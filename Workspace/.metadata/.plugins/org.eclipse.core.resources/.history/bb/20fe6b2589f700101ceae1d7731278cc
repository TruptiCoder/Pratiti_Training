package com.trupti;

public class Student {
	private int age;
	private String name;
	private static int count;
	
	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public static int getCount() {
		return count;
	}

	public static void setCount(int count) {
		Student.count = count;
	}
	
	// Blocks can be multiple in single class
	{
		System.out.println("This is initializer block it runs when a object is created" + ++count);
	}
	
	static {
		System.out.println("This is static block, it will called only once in class is loaded" + ++count);
	}
}
