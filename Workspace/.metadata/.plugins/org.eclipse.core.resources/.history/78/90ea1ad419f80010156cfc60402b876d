package com.trupti;

public class ThingsInClass {
	
	public class A {
		int x1 = 20;
		static String xaa = "Hellow";
		private class AA {
			void am() {
				System.out.println(x1);
			}
		}
	}
	
	int x = 10;
	int y;
	
	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}

	public static int getCount() {
		return count;
	}

	public static void setCount(int count) {
		ThingsInClass.count = count;
	}

	static int count;
	
	public ThingsInClass() {
		count++;
	}
}
